       (list (cons \"\\\\.inc$\" 'bb-mode))
       (list (cons \"\\\\.bbappend$\" 'bb-mode))
       (list (cons \"\\\\.bbclass$\" 'bb-mode))
       (list (cons \"\\\\.conf$\" 'bb-mode))

       (list (cons \"\\\\.lua$\" 'lua-mode))

       (list (cons \"\\\\.\\\\([Nn][Ss][Ii]\\\\)$\" 'nsis-mode))
       (list (cons \"\\\\.\\\\([Nn][Ss][Hh]\\\\)$\" 'nsis-mode))
       
       auto-mode-alist))

