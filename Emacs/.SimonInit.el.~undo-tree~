             '((\"doc\" . \"\\\"c:/Program Files (x86)/Microsoft Office/root/Office16/WINWORD.EXE\\\" %s\"))

             org-file-apps
             ))
      )
  (progn 
    (setq org-file-apps
          (append
           '((\"png\" . \"eog %s\"))
           '((\"pdf\" . \"evince %s\"))
           '((\"svg\" . \"inkscape %s\"))
           '((\"net\" . \"/usr/local/samiam/runsamiam %s\"))
           '((\"xcf\" . \"gimp %s\"))
