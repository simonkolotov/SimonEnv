             '((\"doc\" . \"\\\"c:/Program Files (x86)/OpenOffice.org 3/program/soffice.exe\\\" %s\"))

             org-file-apps
             ))
      )
  (progn 
    (setq org-file-apps
          (append
           '((\"png\" . \"eog %s\"))
           '((\"pdf\" . \"evince %s\"))
           '((\"svg\" . \"inkscape %s\"))
           '((\"net\" . \"/usr/local/samiam/runsamiam %s\"))
           '((\"xcf\" . \"gimp %s\"))
           '((\"giv\" . \"giv %s\"))
           '((\"doc\" . \"libreoffice -norestore %s\"))
           '((\"odt\" . \"libreoffice -norestore %s\"))
           '((\"gnumeric\" . \"gnumeric %s\"))
